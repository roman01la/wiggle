{
  "name": "wiggle",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build:ui": "vite build",
    "build:ui:release": "vite build --minify",
    "build:wasm": "zig build www",
    "build:wasm:debug": "zig build debug",
    "build:wasm:release": "zig build release",
    "build": "bun run build:ui && bun run build:wasm:release",
    "build:dev": "bun run build:ui && bun run build:wasm:debug",
    "dev": "vite build --watch",
    "prod": "bun run build:wasm:release && bun run build:ui:release"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.3.0"
  }
}
